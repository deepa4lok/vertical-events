<?xml version="1.0" encoding="utf-8"?>
<odoo>


    <!--
        Event (Website Event)
    -->
    <record id="event_event_view_form" model="ir.ui.view">
        <field name="name">event.event.view.form.inherit.exhibitor</field>
        <field name="inherit_id" ref="website_event_track.event_event_view_form"/>
        <field name="model">event.event</field>
        <field name="priority" eval="3"/>
        <field name="arch" type="xml">

            <xpath expr="//field[@name='auto_confirm']" position="after">
                <field name="website_exhibitor_reg_url" groups="base.group_no_one"/>
            </xpath>

        </field>
    </record>

</odoo>